{
  "name": "easegen",
  "description": "A library for generating a set of cubic bezier easing functions by providing a min and max points and generating all possible combinations.",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/easegen.umd.cjs",
  "module": "dist/easegen.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/easegen.js",
      "require": "./dist/easegen.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "easing",
    "animation",
    "cubic-bezier",
    "css",
    "javascript",
    "react",
    "framer-motion",
    "transition",
    "timing-function"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "framer-motion": "^11.3.30",
    "@eslint/js": "^9.9.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.41",
    "rollup-preserve-directives": "^1.1.1",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.0.3"
  }
}
